{
    "name": "accelasearch/prestashop-module",
    "description": "Boost your search engine with AI",
    "keywords": [
        "accelasearch",
        "accelasearch"
    ],
    "authors": [
        {
            "name": "AccelaSearch",
            "email": "info@accelasearch.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Accelasearch\\Accelasearch\\": "src"
        }
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3",
        "prestashop/autoindex": "^2.1",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^8.5.34"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "prestashop-module",
    "config" : {
        "prepend-autoloader": false
    },
    "author": "PrestaShop",
    "license": "AFL-3.0",
    "require": {
        "accelasearch/sdk-php": "^0.5.0",
        "vitalybaev/google-merchant-feed": "^2.6"
    }
}
