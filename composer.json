{
    "name": "accelasearch/prestashop-module",
    "description": "Boost your search engine with AI",
    "keywords": [
        "accelasearch",
        "accelasearch"
    ],
    "authors": [
        {
            "name": "AccelaSearch",
            "email": "info@accelasearch.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Accelasearch\\Accelasearch\\": "src"
        }
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3",
        "prestashop/autoindex": "^2.1",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^8.5.34",
        "mockery/mockery": "^1.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "translate" : "php scripts/translate.php",
        "md5allocate" : "php scripts/md5allocate.php",
        "test" : "vendor/bin/phpunit -c tests/Unit/phpunit.xml"
    },
    "type": "prestashop-module",
    "config" : {
        "prepend-autoloader": false
    },
    "author": "PrestaShop",
    "license": "AFL-3.0",
    "require": {
        "vitalybaev/google-merchant-feed": "^2.6",
        "accelasearch/google-merchant-validator": "^0.2.0",
        "accelasearch/sdk-php": "dev-main",
        "ector/release-downloader": "^1.1",
        "guzzlehttp/guzzle": "^7.0"
    }
}
